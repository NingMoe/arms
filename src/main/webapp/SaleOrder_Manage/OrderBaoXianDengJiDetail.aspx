        function GetDate() {
            var NextDate = new Date();
            var strNowDate = document.getElementById("txtBaoXianStartDate").value;
            var arrNowDate = strNowDate.split('-');
            if (arrNowDate.length == 3) {
                strNowDate = arrNowDate[0] + "/" + arrNowDate[1] + "/" + arrNowDate[2];
            }

            var NowDate = new Date(strNowDate);
            NowDate.setYear(NowDate.getFullYear() + 1);
            NowDate.setDate(NowDate.getDate() - 1);
            document.getElementById("txtBaoXianEndDate").value = NowDate.getFullYear() + "-" + (parseInt(NowDate.getMonth()) + 1) + "-" + NowDate.getDate();
            return false;
        }

