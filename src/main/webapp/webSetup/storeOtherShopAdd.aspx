    <script language="javascript" type="text/javascript" >


        function nextBill() {
            location.href = "storeOtherShopAdd.aspx?shopGuid=0&d="+new Date();
            return false;
        }


        var btnFlag = "0";
        function saveCheck(sort) {
            var err = "";
            if (btnFlag == "0") {btnFlag = "1";} else {return false;}

            //必填项验证 start
            if ($("#txtShopGuid").val() == "") { err += "系统发生不可预见性异常！\n"; }
            if ($("#txtShopId").val() == "") { err += "商品编号必须填写！\n"; }
            if ($("#txtShopName").val() == "") { err += "商品名称必须填写！\n"; }
            //if ($("#txtTiaoMa").val() == "") { err += "商品条码必须填写！\n"; }
            //if ($("#txtYouXiaoQi").val() == "") { err += "商品有效期必须填写！\n"; }
            //if ($("#txtChangJiaBrand").val() == "") { err += "厂家品牌必须填写！\n"; }
            if ($("#txtSalePrice").val() == "") { err += "销售价格必须填写！\n"; }
            //if ($("#txtStockPos").val() == "") { err += "库位必须填写！\n"; }
            if ($("#txtKuCunShangXian").val() == "") { err += "库存上限必须填写！\n"; }
            if ($("#txtKuCunXiaXian").val() == "") { err += "库存下限必须填写！\n"; }
            //必填项验证 end
             
            if ($("#txtRemarks").val().length >50 ) {    err +="备注长度过长!\n";}

            if (err != "")
            {btnFlag = "0";alert(err);return false;}


            if (err == "") {
                if (sort == "1") {
                    //保存
                    __doPostBack('btnAdd', '');
                } else {
                    //修改
                    __doPostBack('btnMdf', '');
                }

            } else {
                btnFlag = "0";
                alert(err);
                return false;
            }

            return false;
        }


</script>
