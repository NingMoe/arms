        function changeDiaoCha() 
        {
            if (document.getElementById("chkCustDiaoCha").checked)
                document.getElementById("tbCustDiaoCha").style.display = "block";
            else
                document.getElementById("tbCustDiaoCha").style.display = "none";
        }
        function GetStrings() {
            var oo = document.getElementById("tbCustDiaoCha").getElementsByTagName("input");
            var strCHKKBW = "", strCHKPBT = "", strCHKI = "";
            len = oo.length;
            for (var k = 0; k < len; k++) {
                if (oo[k].type == "checkbox" && oo[k].id.indexOf("chkKBW") >= 0 && oo[k].checked == true)
                    strCHKKBW += strCHKKBW == "" ? oo[k].value : ';' + oo[k].value;
                if (oo[k].type == "checkbox" && oo[k].id.indexOf("chkPBT") >= 0 && oo[k].checked == true)
                    strCHKPBT += strCHKPBT == "" ? oo[k].value : ';' + oo[k].value;
                if (oo[k].type == "checkbox" && oo[k].id.indexOf("chkI") >= 0 && oo[k].checked == true)
                    strCHKI += strCHKI == "" ? oo[k].value : ';' + oo[k].value;
            }
            document.getElementById("hidKnowByWay").value = strCHKKBW;
            document.getElementById("hidPlanBuyTime").value = strCHKPBT;
            document.getElementById("hidInterest").value = strCHKI;
            return true;
        }
        function InitCustDiaoCha() {
            var strKnowByWay = "", strPlanBuyTime = "", strInterest = "";
            strKnowByWay = document.getElementById("hidKnowByWay").value;
            strPlanBuyTime = document.getElementById("hidPlanBuyTime").value;
            strInterest = document.getElementById("hidInterest").value;

            var arrKBWay = strKnowByWay.split(';');
            var arrPLTime = strPlanBuyTime.split(';');
            var arrInter = strInterest.split(';');
            initCheckBoxs(arrKBWay);
            initCheckBoxs(arrPLTime);
            initCheckBoxs(arrInter);
        }

        function initCheckBoxs(arrStr) {
            var obj = document.getElementById("tbCustDiaoCha").getElementsByTagName("input");
            for (var i = 0; i < arrStr.length; i++) {
                for (var j = 0; j < obj.length; j++) {
                    if (arrStr[i] == obj[j].value)
                        obj[j].checked = true;
                }
            }
        }
        window.onload = InitCustDiaoCha;





            </script>
