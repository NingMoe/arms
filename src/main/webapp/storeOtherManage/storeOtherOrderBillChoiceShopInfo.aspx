    <script language="javascript" type="text/javascript" >

        function insertShop(shopThisId) {          

            $.post("storeOtherOrderBillExec.aspx?type=orderDetailShopIdExists&d=" + new Date(), {
                thisBillGuid: '', shopThisId: shopThisId
            }, function (data) {
                var t = data.split('|');
                if (t.length == 2) {
                    if (t[1] != "") {
                        if (!confirm(t[1])) {
                            return false;
                        }
                    }

                    //商品插入操作start
                    $.get("storeOtherOrderBillExec.aspx?type=orderDetailInsert&d=" + new Date(), { thisBillGuid: '', shopThisId: shopThisId }, function (data) {
                        if (data == "ok") { alert('插入成功！'); }
                    });
                    //商品插入操作end


                } else {
                    alert(data);
                }


            });



        }
      
    </script>

